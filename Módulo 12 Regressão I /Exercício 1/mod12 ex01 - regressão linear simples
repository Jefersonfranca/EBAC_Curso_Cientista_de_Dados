   "source": [
    "tips = load_dataset(name='tips')\n",
    "\n",
    "tips['tip_pct'] = tips['tip'] / (tips['total_bill'] - tips['tip'])\n",
    "tips['net_bill'] = tips['total_bill'] - tips['tip']\n",
    "\n",
    "tips = tips[tips['tip_pct']<.5]\n",
    "tips"
   ]
